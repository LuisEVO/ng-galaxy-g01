    "app-s01-data-binding": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/app-s01-data-binding",
      "sourceRoot": "apps/app-s01-data-binding/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-s01-data-binding",
            "index": "apps/app-s01-data-binding/src/index.html",
            "main": "apps/app-s01-data-binding/src/main.ts",
            "polyfills": "apps/app-s01-data-binding/src/polyfills.ts",
            "tsConfig": "apps/app-s01-data-binding/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/app-s01-data-binding/src/favicon.ico",
              "apps/app-s01-data-binding/src/assets"
            ],
            "styles": [
              "apps/app-s01-data-binding/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/app-s01-data-binding/src/environments/environment.ts",
                  "with": "apps/app-s01-data-binding/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app-s01-data-binding:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app-s01-data-binding:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-s01-data-binding:build"
