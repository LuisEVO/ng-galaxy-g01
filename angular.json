            "tsConfig": "apps/app-s01-data-binding/tsconfig.spec.json",
            "karmaConfig": "apps/app-s01-data-binding/karma.conf.js",
            "assets": [
              "apps/app-s01-data-binding/src/favicon.ico",
              "apps/app-s01-data-binding/src/assets"
            ],
            "styles": [
              "apps/app-s01-data-binding/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
